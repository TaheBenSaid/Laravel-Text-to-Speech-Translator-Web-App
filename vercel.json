{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "DB_CONNECTION": "sqlite",
    "SESSION_DRIVER": "database",
    "CACHE_STORE": "database",
    "QUEUE_CONNECTION": "database",
    "LOG_CHANNEL": "stack",
    "LOG_LEVEL": "error"
  },
  "functions": {
    "public/index.php": {
      "runtime": "php@8.2"
    }
  }
}
